        "if (!(Test-Path \"$persist_dir\\license\")) { New-Item \"$dir\\license\" | Out-Null }"
    ],
    "bin": "teracopy.exe",
    "shortcuts": [
        [
            "TeraCopy.exe",
            "TeraCopy"
        ]
    ],
    "persist": [
        "Options.ini",
        "main.db",
        "license"
    ],
    "checkver": {
        "url": "https://codesector.com/downloads",
        "regex": ">TeraCopy ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://www.codesector.com/files/teracopy.zip"
    }
