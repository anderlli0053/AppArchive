  "bin": "dotnet.exe",
  "checkver": {
    "jsonpath": "$.releases-index[?(@.channel-version == '6.0')].latest-release",
    "url": "https://dotnetcli.blob.core.windows.net/dotnet/release-metadata/releases-index.json"
  },
  "depends": "dark",
  "description": "Microsoft .NET 6 Desktop Runtime.",
  "env_set": {
    "DOTNET_ROOT": "$dir"
