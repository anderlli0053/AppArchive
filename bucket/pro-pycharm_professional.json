{
    "version": "2018.2.5",
    "url": "https://download.jetbrains.com/python/pycharm-professional-2018.2.5.zip",
    "env_add_path": "bin",
    "architecture": {
        "64bit": {
            "bin": [
                "bin\\pycharm64.exe",
                [
                    "bin\\pycharm64.exe",
                    "pycharm_pro"
                ]
            ],
            "shortcuts": [
                [
                    "bin/pycharm64.exe",
                    "IDEA PyCharm Professional"
                ]
            ]
        },
        "32bit": {
            "bin": "bin\\pycharm.exe",
            "shortcuts": [
                [
                    "bin/pycharm.exe",
                    "IDEA PyCharm Professional"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PCP&latest=true&type=release",
        "jp": "$..version"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/python/pycharm-professional-$version.zip",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
