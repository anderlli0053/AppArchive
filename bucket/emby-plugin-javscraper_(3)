  "installer": {
    "script": [
      "if ($env:CI -eq \"true\") { Exit }",
      "if (installed 'keepass' $global) {Copy-Item \"$dir\\KeePassOTP.plgx\" \"$(appdir keepass $global)\\current\\Plugins\"} else { error 'Please install `keepass` first, then reinstall `keepass-plugin-keepassotp`.'}"
    ]
