      "# Convert .pack to .jar, and remove .pack",
      "pushd \"$dir\"",
      "ls \"$dir\" -recurse | ? name -match '^[^_].*?\\.(?i)pack$' | % {",
      "    $name = $_.fullname -replace '\\.(?i)pack$', ''",
      "    $pack = \"$name.pack\"",
      "    $jar = \"$name.jar\"",
      "    & \"bin\\unpack200.exe\" \"-r\" \"$pack\" \"$jar\"",
