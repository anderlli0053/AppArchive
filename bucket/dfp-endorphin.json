  "installer": {
    "script": [
      "$vst2dir = $env:LOCALAPPDATA + \"\\programs\\common\\vst2\"",
      "Write-Host VST2 folder $vst2dir",
      "New-Item -Force -ItemType Directory -Path $vst2dir | Out-Null",
